#!/bin/bash

base=$(cd ${0%/*}/../..; pwd)

file=$1

if [ -z $file ]; then
        echo "Need to provide the file"                                     
        exit
fi   

filename=$(basename $file)

for i in {1..23} 
do
	bin/load/submit_chr.sh $filename $i
	bin/load/submit_snp.sh $filename $i
done
$base/bin/load/submit_trait.sh $filename
